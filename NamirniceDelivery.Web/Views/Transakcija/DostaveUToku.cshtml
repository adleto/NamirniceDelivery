@model NamirniceDelivery.Web.ViewModels.Transakcija.NarudzbeViewModel
@{
    ViewData["Title"] = "Dostave U Toku";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="text-center row justify-content-center">
    <div>
        <h1>Dostave u toku</h1>
        <br />
        @if (Model.TransakcijaList.Any())
        {
            <partial name="_PregledTransakcijaPartial" model="@(new NamirniceDelivery.Web.ViewModels.Shared.PregledTransakcijaPartialViewModel { TransakcijaList = Model.TransakcijaList, ListType = NamirniceDelivery.Web.ViewModels.Shared.ListType.prihvacene })" />
        }
        else
        {
            <p>
                <span>Nema dostava u toku</span>
            </p>
        }
    </div>
</div>

